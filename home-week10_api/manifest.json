{
    "manifest_version": 3,
    "name": "Random Quiz Generator",
    "description": "Test your knowledge with random quizzes!",
    "version": "1.0",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_popup_size": {
        "width": 400,
        "height": 800
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  